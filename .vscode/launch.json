  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'hamming-encoder'",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=hamming-encoder"],
        "filter": {
          "name": "hamming-encoder",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'hamming-encoder'",
      "cargo": {
        "args": ["build", "--bin=hamming-encoder", "--package=hamming-encoder"],
        "filter": {
          "name": "hamming-encoder",
          "kind": "bin"
        }
      },
      "args": ["./encoder/examples/hello.txt"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'hamming-encoder'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=hamming-encoder",
          "--package=hamming-encoder"
        ],
        "filter": {
          "name": "hamming-encoder",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'hamming-decoder'",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=hamming-decoder"],
        "filter": {
          "name": "hamming-decoder",
          "kind": "lib"
        }
      },
